

<div class="gb-breadcrumb gb-bg white-color">
	<div class="container">
		<div class="breadcrumb-info text-center">
			<div>
				<h1>
					<span class="before-top"></span>
					<span>Parceiros</span>
					<span class="before-bottom"></span>
				</h1>
			</div>
			<ol class="breadcrumb">
				<li><%=link_to 'Parceiros', partners_path %></li>
				<li class="active"> <%= link_to 'Contato', contact_path %></li>
			</ol>				
		</div>
	</div><!-- /.container -->
</div><!-- /.gb-breadcrumb -->		
<div class="gb-speakers gb-section">
	<div class="container">
	<% @category_partners.each do |category_partner|%>
		<div class="title-index">
			<h1 class="color-red">
				<span class="before-top"></span>
				<span><%= category_partner.name %></span>
				<span class="before-bottom"></span>
			</h1>
		</div>		



		<div class="row">
			<% category_partner.crew_partners.each do |partner| %>
			<div class="col-sm-6 col-md-3">
				<div class="speaker">
					<div class="speaker-image">
						<%= image_tag partner.photo_url(:partners) %>
					</div>
					<div class="speaker-title">
						<h2><%= partner.name%></h2>

					</div>
				</div>
			</div>
			<%end %>

		</div>

		<%end %>
	</div>
</div>