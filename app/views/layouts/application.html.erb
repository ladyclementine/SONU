<!DOCTYPE html>
<html>
  <head>
    <title>SONU</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
            	<% flash.each do |name, msg| %>
          <div class='alert alert-<%= name %>'>
          <%= msg %>
          </div>
          <% end %>
    <%= yield %>
    
  </body>
</html>
