<div class="row">
   <div class="col-sm-12">
      <div class="card-box table-responsive">
   
      <%=link_to new_crew_comitee_path do %><button type="button" class="btn btn-primary waves-effect w-md waves-light m-b-5 pull-right">Adicionar Comitê</button><% end %>
         <h4 class="header-title m-t-0 m-b-30">Gerenciamento de Comitês</h4>
         <table id="datatable-buttons" class="table table-striped table-bordered">
            <thead>
              <tr>
                <th>Activity</th>
                <th>Start</th>
                <th>End</th>
                <th colspan="3"></th>
              </tr>
            </thead>

            <tbody>
              <% @crew_big_sonu_schedules.each do |crew_big_sonu_schedule| %>
                <tr>
                  <td><%= raw crew_big_sonu_schedule.activity %></td>
                  <td><%= crew_big_sonu_schedule.start.strftime("%d/%m/%Y -  %H:%M") %></td>
                  <td><%= crew_big_sonu_schedule.end.strftime("%d/%m/%Y - %H:%M") %></td>
                  <td><%= link_to 'Show', crew_big_sonu_schedule %></td>
                  <td><%= link_to 'Edit', edit_crew_big_sonu_schedule_path(crew_big_sonu_schedule) %></td>
                  <td><%= button_to 'Destroy', crew_big_sonu_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                </tr>
              <% end %>
            </tbody>
          </table>

          <br>
            </tbody>
         </table>
      </div>
   </div>
   <!-- end col -->
</div>
<!-- end row -->
          <%= link_to 'New Crew_big_sonu_schedule', new_crew_big_sonu_schedule_path %>
