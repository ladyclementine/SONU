<div class="row">
   <div class="col-md-12">
      <div class="card-box">
         <div class="dropdown pull-right">
            <a href="#" class="dropdown-toggle card-drop" data-toggle="dropdown" aria-expanded="false">
            <i class="zmdi zmdi-more-vert"></i>
            </a>
            <ul class="dropdown-menu" role="menu">
               <li><a href="#">Action</a></li>
               <li><a href="#">Another action</a></li>
               <li><a href="#">Something else here</a></li>
               <li class="divider"></li>
               <li><a href="#">Separated link</a></li>
            </ul>
         </div>
         <h4 class="header-title m-t-0 m-b-30">Hstória da SONU</h4>
            <table  class="table m-0">
            <thead>
               <tr>
                  <th>Título</th>
                  <th>Descrição</th>
                  <th colspan="2">Configurações</th>
               </tr>
            </thead>
            <tbody>
              <% @crew_solidaries.each do |crew_solidary| %>
                <tr>
                  <td><%= crew_solidary.title %></td>
                  <td><%= crew_solidary.description %></td>
                  <td><%= crew_solidary.photo %></td>
                  <td><%= link_to 'Show', crew_solidary %></td>
                  <td><%= link_to 'Edit', edit_crew_solidary_path(crew_solidary) %></td>
                  <td><%= link_to 'Destroy', crew_solidary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                </tr>
              <% end %>
            </tbody>
          </table>

          <br>

          <%= link_to 'Novo conteúdo', new_crew_solidary_path, class: 'btn btn-primary' %>
      </div>
   </div>
   <!-- end col -->
</div>