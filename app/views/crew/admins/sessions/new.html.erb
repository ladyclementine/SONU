          <div class="account-pages-login"></div>
          <div class="clearfix"></div>
          <div class="wrapper-page">
             <div class="text-center">
                <div class="col-md-12">
                   <%= image_tag("sonu-login.png", :alt => "SONU",:class => "profile-image-login") %>
                   <p class="text-center">Painel Administrativo</p>
                </div>
             </div>
             <div class="m-t-40 card-box">
                <div class="text-center">
                   <h4 class="text-uppercase font-bold m-b-0">Login</h4>
                </div>
                <div class="panel-body">
                   <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: "form-horizontal m-t-20" } ) do |f| %>
                   <div class="form-group">
                      <div class="col-xs-12">
                         <%= f.email_field :email, autofocus: true, class: 'form-control', placeholder: 'exemplo@mail.com' %>
                      </div>
                   </div>
                   <div class="form-group">
                      <div class="col-xs-12">
                         <%= f.password_field :password, autocomplete: "off", class: 'form-control', placeholder: 'senha' %>
                      </div>
                   </div>
                   <!--
                      <% if devise_mapping.rememberable? -%>
                        <div class="form-group">
                              <div class="checkbox checkbox-primary">
                          <%= f.check_box :remember_me %>
                          <%= f.label :remember_me %>
                        </div>
                          </div>
                      <% end -%>
                      -->
                   <div class="form-group text-center m-t-30">
                      <div class="col-xs-12">
                         <%= f.submit "Entrar", class: 'btn btn-custom' %>
                      </div>
                   </div>
                   <% end %>
                </div>
             </div>
             <!-- end card-box-->
             <!-- <div class="row">
                <div class="col-sm-12 text-center">
                    <p class="text-muted">Don't have an account? <a href="page-register.html" class="text-primary m-l-5"><b>Sign Up</b></a></p>
                </div>
                </div> -->
          </div>
          <!-- end wrapper page -->