<div class="row">
   <div class="col-md-12">
      <div class="card-box">
         <div class="dropdown pull-right">
            
          <%= link_to 'Adicionar comitê', new_crew_big_sonu_comitee_path,class: 'btn btn-primary' %>
         </div>
         <h4 class="header-title m-t-0 m-b-30">Comitês SONU grande</h4>
            <table  class="table m-0">
            <thead>
               <tr>
                  <th>Título</th>
                  <th>Configurações</th>
               </tr>
            </thead>
            <tbody>
              <% @crew_big_sonu_comitees.each do |crew_big_sonu_comitee| %>
                <tr>
                  <td><%= crew_big_sonu_comitee.name %></td>
                  <td><%= link_to   crew_big_sonu_comitee do %><button data-toggle="tooltip" data-placement="left" title="Ver mais" class="btn btn-icon waves-effect waves-light btn-primary m-b-5 __web-inspector-hide-shortcut__"> <i class="fa fa-eye"></i></button><% end %>
                 <%=  link_to  edit_crew_big_sonu_comitee_path(crew_big_sonu_comitee) do %>
                 <button data-toggle="tooltip" data-placement="left" title="Editar" class="btn btn-icon waves-effect waves-light btn-warning m-b-5"> <i class="fa fa-wrench"></i></button><% end %>

                 <%= link_to crew_big_sonu_comitee, :method=>:delete, :class=>:destroy, data: { confirm: 'Tem certeza que deseja apagar?' } do %><button data-toggle="tooltip" data-placement="left" title="Apagar" class="btn btn-icon waves-effect waves-light btn-danger m-b-5 __web-inspector-hide-shortcut__"> <i class="fa fa-remove"></i></button><% end %></td>
                </tr>
              <% end %>
            </tbody>
          </table>

          <br>

      </div>
   </div>
   <!-- end col -->
</div>